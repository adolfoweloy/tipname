{
  "name": "tipname",
  "version": "1.0",
  "description": "Extension that allows one to find what an acronym stands for given a custom database",
  "permissions": ["contextMenus", "storage"],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Sync",
    "default_popup": "settings/popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2
}
